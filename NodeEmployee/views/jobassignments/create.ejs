<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link href="/images/favicon.ico" rel="shortcut icon" type="image/png">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css" rel="stylesheet" />
    <link href="/stylesheets/light-bootstrap-dashboard.css?v=1.4.0" rel="stylesheet" />
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
	<link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/jquery.bootstrapvalidator/0.5.3/css/bootstrapValidator.min.css"/>
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/jquery.bootstrapvalidator/0.5.3/js/bootstrapValidator.min.js"> </script>
	
	
	
</head>
<body>
    <div class="wrapper">
        <div class="sidebar" data-color="purple" data-image/image/sidebar-5.jpg>
		      <div class="sidebar-wrapper">
		
                    <div class="logo">
                        <a href="http://www.safe-lanes.com" class="simple-text">
                            Safe Lanes
                        </a>
                    </div>
                    <ul class="nav">
                        <li>
                            <a href="/dashboard">
                               <p>Dashboard</p>
                            </a>
                        </li>
                        
					    <li>
                           <a href="/user">
                              <p>User Profile</p>
                           </a>
                        </li>
						
						<li>
                            <a href="/employees">
                               <p>Employee Master</p>
                            </a>
                        </li>
						
                        <li>
                            <a href="/vdranalysis">
                                <p>VDR Analysis</p>
                            </a>
                        </li>
                        <li  class="active">
                            <a href="/jobassignments">
                               <p>Job Assignment</p>
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
        
 <div class="main-panel">
    <nav class="navbar navbar-default navbar-fixed">
        <div class="container-fluid">
            <div  align="right">
                <a href="">Account</a>
                   &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
                <a href="#">Log out</a>
            </div>
        </div>
	</nav>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-10">
                        <div class="card">
                            <div class="header">
							   
                                  <h5><a href="/jobassignments">Job Assignment Tracker</a></h5>
                                  <h5>Create New Assignment</h5>
						          <div align="right">
							         <h5><a href="/jobs/create">Add/Edit Jobs</a></h5>
								  </div>
							   
							</div>
							<div class="content">
                               <form id ="createjobassignment" action="/jobassignments/save" enctype="multipart/form-data" method="post">
							
							     <div class="row">
                                   <div class="col-md-4">
                                      <div class="form-group">
                                           <label>Auditor *</label>
                                                 <select name="employeename" class="form-control">
                                                  <% for(var i=0; i < employee.length; i++){ %>
                                                   <option value='<%= employee[i].employee_id %>'><%= employee[i].firstname %></li>
                                                   <% } %>
                                                   </select>
									   </div>
                                   </div>
                                   <div class="col-md-4">
                                      <div class="form-group">
                                           <label>Job Type *</label>
                                                <select name="jobtype" class="form-control">
												<% for(var i=0; i < job.length; i++){ %>
								                 <option value='<%= job[i].job_id %>'><%= job[i].jobtype %></li>
                                                 <% } %>
                                                 </select>
                                      </div>
                                   </div>
                                 </div>
								 
								 <div class="row">
                                   <div class="col-md-4">
                                      <div class="form-group">
                                           <label>Vessel *</label>
                                                  <select name="vessel" class="form-control">
												  <% for(var i=0; i < vessel.length; i++){ %>
								                 <option value='<%= vessel[i]._id %>'><%= vessel[i].name %></li>
                                                 <% } %>
                                                 </select>
									   </div>
                                   </div>
                                   <div class="col-md-4">
                                      <div class="form-group">
                                           <label>Company *</label>
                                                <select name="company" class="form-control">
								                 <% for(var i=0; i < company.length; i++){ %>
								                 <option value='<%= company[i]._id %>'><%= company[i].name %></li>
                                                 <% } %>
                                                 </select>
                                      </div>
                                   </div>
                                 </div>
								 
								 <div class="row">
                                   <div class="col-md-4">
                                      <div class="form-group">
                                           <label>Assigned Date *</label>
                                                 <input type="date" class="form-control" placeholder="Assigned Date" name="assigneddate">
									   </div>
                                   </div>
                                   <div class="col-md-4">
                                      <div class="form-group">
                                           <label>Travel Date *</label>
                                               <input type="date" class="form-control" placeholder="Travel Date" name="traveldate">
                                      </div>
                                   </div>
                                 </div>
								 
								 <div class="row">
                                   <div class="col-md-4">
                                      <div class="form-group">
                                           <label>Destination from *</label>
                                                  <select name="destfrom" class="form-control">
								                   <% for(var i=0; i < country.length; i++){ %>
								                   <option value='<%= country[i]._id %>'><%= country[i].name %></li>
                                                   <% } %>
							                      </select>
									   </div>
                                   </div>
                                   <div class="col-md-4">
                                      <div class="form-group">
                                           <label>Destination To *</label>
                                                <select name="destto" class="form-control">
								                 <% for(var i=0; i < country.length; i++){ %>
								                   <option value='<%= country[i]._id %><%= country[i].name %>'><%= country[i].name %></li>
                                                   <% } %>
								                </select>
                                      </div>
                                   </div>
                                 </div>
								 
								 <div class="row">
								    <div class="col-md-4">
						               <div class="form-group">
                                           <label>Upload SEQ</label>
                                           <input type="file" class="form-control-file" name="doc" id="doc"/>
										</div>
									</div>
						        </div>
								 
								 <button type="submit" class="btn btn-info btn-fill pull-right" onclick="abc();">Save</button>
							    <div class="clearfix"></div>
                            </div>
							
                           </form>
                            </div>
                       </div>
                    </div>
			    </div>
         </body>
</html>

<script type="text/javascript">

function abc()
{
var a = document.getElementById("employee").value;
var b = document.getElementById("employee").text;
alert(a);
alert(b);
}



	$(document).ready(function() {
    $('#createjobassignment').bootstrapValidator({
        container: '#messages',
        feedbackIcons: {
            valid: 'glyphicon glyphicon-ok',
            invalid: 'glyphicon glyphicon-remove',
            validating: 'glyphicon glyphicon-refresh'
        },
        fields: {
            employeename: {
                validators: {
                    notEmpty: {
                        message: ''
                    }
                }
            },
            jobtype: {
                validators: {
                    notEmpty: {
                        message: ''
                    }
                }
            },
			 vessel: {
                validators: {
                    notEmpty: {
                        message: ''
                    }
                }
            },
            company: {
                validators: {
                    notEmpty: {
                        message: ''
                    }
                }
            },
			assigneddate: {
                validators: {
                    notEmpty: {
                        message: ''
                    }
                }
            },
            traveldate: {
                validators: {
                    notEmpty: {
                        message: ''
                    }
                }
            },
			destfrom: {
                validators: {
                    notEmpty: {
                        message: ''
                    }
                }
            },
            destto: {
                validators: {
                    notEmpty: {
                        message: ''
                    }
                }
            }
        }
    });
	
	
		
	
});


   

</script> 
			
						 
						  
						
						  
						
			
			
			
			     
			



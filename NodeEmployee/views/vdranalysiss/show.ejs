<script type="text/javascript">

 
 jQuery( document ).ready(function( $ ) {
  
  
  $('#remarks').val('<%= vdranalysis.remarks %>');
  
  
 if('<%=vdranalysis.status%>'== 'completed'){
 
      $(':input[type="submit"]').prop('disabled', true);
	  $('#btncancel').addClass('disabled');
	  $('#edit12').addClass('disabled');
      }
  if('<%=vdranalysis.status%>' == 'cancelled'){
 
       
	  $('#edit12').addClass('disabled');
	  $('#btncancel').addClass('disabled');
	  $(':input[type="submit"]').prop('disabled', true);
      }
  
  if('<%=vdranalysis.vdrtype%>'!=null)
       {
           
		   if('<%=vdranalysis.vdrtype%>' == 'routineanalysis'){
              document.getElementById('contactChoice1').checked=true;
			 
             }
          else
           {
             document.getElementById('contactChoice2').checked=true;
			
             
           }
		     document.getElementById('contactChoice1').disabled=true;
			 document.getElementById('contactChoice2').disabled=true;
       }
     var dt1 = '<%= vdranalysis.drcvclient %>';
	 d1 = new Date(dt1);
	 $('#drcvclient').val($.datepicker.formatDate('yy-mm-dd', d1));
	 
	 var dt2 = '<%= vdranalysis.alldocrcvd %>';
	 d2 = new Date(dt2);
	 $('#alldocrcvd').val($.datepicker.formatDate('yy-mm-dd', d2));
	 
	 var dt3 = '<%= vdranalysis.dtdatasent %>';
	 d3 = new Date(dt3);
	 $('#dtdatasent').val($.datepicker.formatDate('yy-mm-dd', d3));
	 
	 var dt4 = '<%= vdranalysis.dtanacomp %>';
	 d4 = new Date(dt4);
	 $('#dtanacomp').val($.datepicker.formatDate('yy-mm-dd', d4));
	 
	 var dt5 = '<%= vdranalysis.dtassign %>';
	 d5 = new Date(dt5);
	 $('#dtassign').val($.datepicker.formatDate('yy-mm-dd', d5));
	 
	 var dt6 = '<%= vdranalysis.dtdeliver %>';
	 d6 = new Date(dt6);
	 $('#dtdeliver').val($.datepicker.formatDate('yy-mm-dd', d6));
	 
	 var dt7 = '<%= vdranalysis.dtcomp %>';
	 d7 = new Date(dt1);
	 $('#dtcomp').val($.datepicker.formatDate('yy-mm-dd', d7));
	
	 var dt8 = '<%= vdranalysis.duedate %>';
	 d8 = new Date(dt8);
	 $('#duedate').val($.datepicker.formatDate('yy-mm-dd', d8));
});

$("#btncancel").click(function (event) {
		 var check = confirm('Are you sure want to cancel this VDR?');
			 if (check == true) {
			 var itemid = '<%= vdranalysis._id%>';
			   $.ajax({
				type: "POST",
				url: "/vdranalysiss/cancel/" + itemid,
				data: {},
				processData: false,
				contentType: false,
				cache: false,
				timeout: 600000,
				success: window.location.replace("/vdranalysiss"),
				error: function (e) {

					   console.log("ERROR : ", e);
				}
			});
			 
			 }
			 else{
			   return false;
			 }
		});
					
</script>		

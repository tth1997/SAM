                        <div class="content">
						
						<div class="row">
                                  <div class="col-md-4">
                                      <div class="form-group">
                                           <label>Company *</label>
										   <select name="company" class="form-control" id="company" disabled>
												  <option selected value='<%= vdranalysis.company_id %>'><%= vdranalysis.company %></option>
                                                 

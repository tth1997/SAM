                                           <input type="file" class="form-control-file" disabled name="files[]" multiple/>
										</div>
									</div>
						        </div>
								
								<div class="row">
								 <div class="col-md-4">
						               <div class="form-group">
								              <% for(var i=0; i < listofdoc.length; i++){ %>
											  <% if(listofdoc[i].typeofdoc == "SEQ") {%>
				                                <a href="<%= listofdoc[i].filepath %>" target ="_blank"><%= listofdoc[i].document %></a></br> 
                                             <%}}%>								 
							           </div>
								  </div>
								  <div class="col-md-4">
						               <div class="form-group">
											  <% for(var i=0; i < listofdoc.length; i++){ %>
											  <% if(listofdoc[i].typeofdoc != "SEQ") {%>
				                               <a href="<%= listofdoc[i].filepath %>" target ="_blank"><%= listofdoc[i].document %></a></br> 
                                             <%}}%>				 
							           </div>
								  </div>
							    </div>
								
								 
						        <h3><a id="edit12" href="/vdranalysiss/edit/<%= vdranalysis._id%>" class="btn btn-info btn-fill pull-right">EDIT</a></h3>
                                <form action="/vdranalysiss/complete/<%= vdranalysis._id%>" method="post">
                                <button type="submit" class="btn btn-info btn-fill pull-right" id="btnComplete" onclick="return confirm('Are you sure want to do?')">Complete VDR</button>
							    <input id="btncancel" type="button" class="btn btn-info btn-fill pull-right" value ="Cancel VDR"/>
						       <div class="clearfix"></div>
						</div>
						   </form>
                            </div>
                       </div>
                    </div>
			    </div>

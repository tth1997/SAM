  <script>
    $(function() { 
      $('.editlink').on('click', function(){
        var id = $(this).data('id');
        $.ajax({
          method: "GET",
          url: "/jobs/fetchdata",
          data: { id:  id },
        }).done(function( data ) {
          //var result = $.parseJSON(data);
		  $('#id').val(data[0]['_id']);
          $('#jobcategory').val(data[0]['jobcategory']);
          $("#subbtn").val('Edit JobCategory');
          $('#form1').attr('action', '/jobs/edit');
        });
      });
	  
	  $('.editlink1').on('click', function(){
        var id = $(this).data('id');
        $.ajax({
          method: "GET",
          url: "/jobs/fetchdatajob",
          data: { id:  id },
        }).done(function( data ) {
          //var result = $.parseJSON(data);
		  $('#id1').val(data[0]['_id']);
		  $('#jobcategory1 option:selected').removeAttr('selected');
		  $('#jobcategory1 option').map(function () {
                if ($(this).val() == data[0].jobcategory_id) return this;
            }).attr('selected', 'selected');
		  $('#jobcategory1 option:not(:selected)').prop('disabled', true);
		  $('#jobtype').val(data[0]['jobtype']);
		  $('#jobdescription').val(data[0]['jobdescription']);
          $("#subbtn1").val('Edit JobType');
          $('#form2').attr('action', '/jobs/editjob');
        });
      });
	  
    });
	
	
	
	
  </script>

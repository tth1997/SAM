	<script type="text/javascript">

 
 jQuery( document ).ready(function( $ ) {
   
	 var dt1 = '<%= observationanalysis.reviewdate %>';
	 d1 = new Date(dt1);
	 $('#reviewdate').val($.datepicker.formatDate('yy-mm-dd', d1));
	
    <% for (var j = 0 ; j < observationanalysis.nonationality ;j++){ %>
	
	var newDiv=document.createElement('div');
    var html = '<select id ="nationality" name="nationality" class="form-control" disabled>', i ;
    <%for(i = 0; i < nationality.objNat.length; i++) {%>
        html += "<option value='<%= nationality.objNat[i] %>'><%= nationality.objNat[i] %></li>";
		
    <%}%>
	html += "<option selected value='<%= observationanalysis.nationality[j] %>'><%= observationanalysis.nationality[j] %></option>"
    html += '</select>';
	
    newDiv.innerHTML= html;
	
	newDiv.classList.add("form-group");
    document.getElementById('select-container').appendChild(newDiv);
     <%}%>
    
  
  
  
  
});



</script>							

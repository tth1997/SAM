<script type="text/javascript">

	jQuery( document ).ready(function( $ ) {
	
	 $('#vessel option:not(:selected)').prop('disabled', true);
	 $('#company option:not(:selected)').prop('disabled', true);
	 $('#jobcategory option:not(:selected)').prop('disabled', true);
	 $('#jobtype option:not(:selected)').prop('disabled', true);
	
	
	 var dt1 = '<%= observationanalysis.reviewdate %>';
	 d1 = new Date(dt1);
	 $('#reviewdate').val($.datepicker.formatDate('yy-mm-dd', d1));
	
	$country = $('#country');
    $city = $('#city');
	
	$country.change(function() {
    
    $city.html(''); // Clear existing options in second dropdown
	
	 
     var country_name = document.getElementById("country").value;
	if('<%= observationanalysis.country %>' != '' || country_name != ''){
	 
        $.ajax({
          method: "GET",
          url: "/observationanalysis/returncity",
          data: { country_name: country_name },
		  dataType: "json",
        }).done(function( data ) {

          var result = data;
		  
            $.each(result, function (data) {
            var appenddata = "<option value = '" + result[data] + " '>" + result[data] + " </option>";
            $('#city').html($('#city').html() + appenddata);
             });
            
			
			
			if(country_name == '<%= observationanalysis.country %>')
			{
			$('#city').val('<%= observationanalysis.city %>').attr("selected", "selected");
		    }
			
           
      });
	 }
	 
	 else
	 {
	   
		
	   $('#city').append($('<option>', {
			value: '',
			text: 'Select City'
		}));
		
		
	 }
	
    }).change();
	
	
	 $("#nonationality").keypress(function (e) {
     //if the letter is not digit then display error and don't type anything
     if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
        //display error message
        $("#errmsg").html("Digits Only").show().fadeOut("slow");
               return false;
    }
	
	
     });
	 
	 <% for (var j = 0 ; j < observationanalysis.nonationality ;j++){ %>
	
	var newDiv=document.createElement('div');
    var html = '<select id ="nationality" name="nationality" class="form-control">', i ;
    <%for(i = 0; i < nationality.objNat.length; i++) {%>
        html += "<option value='<%= nationality.objNat[i] %>'><%= nationality.objNat[i] %></li>";
		
    <%}%>
	html += "<option selected value='<%= observationanalysis.nationality[j] %>'><%= observationanalysis.nationality[j] %></option>"
    html += '</select>';
	
    newDiv.innerHTML= html;
	
	newDiv.classList.add("form-group");
    document.getElementById('select-container').appendChild(newDiv);
     <%}%>
	 
	$('#nonationality').change(function () {
    document.getElementById('select-container').innerHTML = "";
	
	for (var j = 0 ; j < $('#nonationality').val() ;j++){
	
	addSelect('select-container');
	
	}
    });
	
	
	 
	function addSelect(divname) {
	
	
    var newDiv=document.createElement('div');
    var html = '<select id ="nationality" name="nationality" class="form-control">', i;
    <%for(i = 0; i < nationality.objNat.length; i++) {%>
        html += "<option value='<%= nationality.objNat[i] %>'><%= nationality.objNat[i] %></li>";
    <%}%>
    html += '</select>';
	
    newDiv.innerHTML= html;
	newDiv.classList.add("form-group");
    document.getElementById(divname).appendChild(newDiv);
    }
	
    $('#editobsanalysis').bootstrapValidator({
        container: '#messages',
        feedbackIcons: {
            valid: 'glyphicon glyphicon-ok',
            invalid: 'glyphicon glyphicon-remove',
            validating: 'glyphicon glyphicon-refresh'
        },
        fields: {
            reviewdate: {
                validators: {
                    notEmpty: {
                        message: ''
                    }
                }
            }
        }
    });
});
   

</script> 

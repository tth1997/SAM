                                      <div class="form-group">
                                           <label>No. Of Nationality</label>
                                               <input type="text" class="form-control" placeholder="No. Of Nationality" id="nonationality" name="nonationality" value="<%= observationanalysis.nonationality %>">
											   &nbsp;<span id="errmsg"></span>
                                      </div>
                                   </div>
								   </div>
								 
								  
								 
